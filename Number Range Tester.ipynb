   "source": [
    "def Range(x): \n",
    "    if x < 100:\n",
    "        return 'Within 100'\n",
    "    elif 100 < x < 1000: \n",
    "        return 'Within 1000'\n",
    "    elif 1000 < x < 2000: \n",
    "        return 'Within 2000'\n",
    "    else: \n",
    "        return 'Beyond 2000'\n",
    "n = float(input(\"Enter a number:\"))\n",
    "Range(n)"
   ]
  }
